{
  "Dhcp4": {
    "authoritative": true,
    "interfaces-config": {
      "interfaces": [ "*" ],
      "dhcp-socket-type": "raw"
    },
    
    "control-socket": {
      "socket-type": "unix",
      "socket-name": "/kea/sockets/kea4-ctrl-socket"
    },
    
    "hooks-libraries": [
      {
        "library": "/usr/local/lib/kea/hooks/libdhcp_pgsql.so"
      },
      {
        "library": "/usr/local/lib/kea/hooks/libdhcp_host_cmds.so"
      },
      {
        "library": "/usr/local/lib/kea/hooks/libdhcp_lease_cmds.so"
      },
      {
        "library": "/usr/local/lib/kea/hooks/dhcp_dynamic_subnet.so"
      }
    ],
    
    "lease-database": {
      "type": "memfile",
      "persist": true,
      "name": "/kea/leases/kea-leases4.csv",
      "lfc-interval": 3600
    },
    
    "hosts-database": {
      "type": "postgresql",
      "name": "captive_portal",
      "user": "portal_user",
      "password": "change_this_password",
      "host": "127.0.0.1",
      "port": 5432
    },
    
    "expired-leases-processing": {
      "reclaim-timer-wait-time": 10,
      "flush-reclaimed-timer-wait-time": 25,
      "hold-reclaimed-time": 3600,
      "max-reclaim-leases": 100,
      "max-reclaim-time": 250,
      "unwarned-reclaim-cycles": 5
    },
    
    "renew-timer": 900,
    "rebind-timer": 1800,
    "valid-lifetime": 86400,
    
    "option-data": [
      {
        "name": "domain-name",
        "data": "blackfriars.local"
      },
      {
        "name": "domain-name-servers",
        "data": "192.168.99.4"
      }
    ],
    
    "subnet4": [
      {
        "subnet": "192.168.10.0/24",
        "id": 10,
        "pools": [
          {
            "pool": "192.168.10.6 - 192.168.10.254"
          }
        ],
        "interface": "eth0.10",
        "option-data": [
          {
            "name": "routers",
            "data": "192.168.10.1"
          }
        ],
        "reservations": []
      },
